{
  "name": "discord-moderation-tool",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "esbuild index.ts --bundle --platform=node --outfile=dist/index.js --target=node18",
    "start": "node dist/index.js",
    "dev": "pnpm run build && node --watch dist/index.js",
    "dev:build": "esbuild index.ts --bundle --platform=node --outfile=dist/index.js --target=node18 --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "biome lint .",
    "lint:fix": "biome lint --fix .",
    "format": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "keywords": [],
  "author": "@wiktoriavh",
  "license": "MIT",
  "packageManager": "pnpm@10.17.1",
  "dependencies": {
    "discord.js": "^14.22.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/node": "^24.5.2",
    "esbuild": "^0.25.10",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.1",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "biome format --write",
      "biome lint --fix"
    ]
  }
}
